<%= form_with(model: @van) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.label :price %>
  <%= form.number_field :price %>

  <%= form.label :model %>
  <%= form.text_field :model %>

  <%= form.label :seats %>
  <%= form.number_field :seats %>

  <%= form.label :size %>
  <%= form.text_field :size %>

  <%= form.label :photos %>
  <%= form.file_field :photos, multiple: true %>

  <%= form.submit %>
<% end %>
